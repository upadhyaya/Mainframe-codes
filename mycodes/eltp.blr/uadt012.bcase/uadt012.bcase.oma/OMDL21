      *         END-EVALUATE.                                           00547700
        2500-PAGEDOWN-PARA.                                             00547800
      *         MOVE 13 TO  ORDER-ID                                    00547900
                IF LS-ORDER-ID(8) IS NOT EQUAL TO SPACES OR             00548000
                        LS-ORDER-ID(8) IS NOT EQUAL TO LOW-VALUES OR    00548100
                        LS-ORDER-ID(8) IS NOT EQUAL TO 0                00548200
                        MOVE Q-LAST-REC TO ORDER-ID                     00548300
                ELSE                                                    00548400
                        MOVE 'END OF PAGE' TO LS-MSG                    00548500
                        EXEC CICS RETURN END-EXEC                       00548600
                END-IF                                                  00548700
      *         MOVE 13 TO  ORDER-ID                                    00548800
      *         MOVE 2  TO  DEALER-ID                                   00548900
                SET K TO 1.                                             00549000
                DISPLAY 'PGDNORDER ID ' ORDER-ID                        00549100
                EXEC SQL OPEN OM2 END-EXEC.                             00549200
                PERFORM 2000-FETCH-FWD-PARA UNTIL SQLCODE = 100         00549300
                    OR K > 8                                            00549400
                EXEC SQL CLOSE OM2 END-EXEC.                            00550000
                PERFORM 3000-WRITEQ-PARA                                00551000
                EXEC CICS RETURN END-EXEC.                              00560000
         2500-PAGEUP-PARA.                                              00570000
                IF LS-ORDER-ID(1) IS NOT EQUAL TO SPACES OR             00580000
                        LS-ORDER-ID(1) IS NOT EQUAL TO LOW-VALUES       00590000
                        MOVE Q-FIRST-REC TO ORDER-ID                    00600000
                ELSE                                                    00610000
                        MOVE 'END OF PAGE' TO LS-MSG                    00620000
                        EXEC CICS RETURN END-EXEC                       00630000
                END-IF                                                  00640000
                SET K TO 8.                                             00650000
                EXEC SQL OPEN OM3 END-EXEC.                             00670000
                PERFORM 2000-FETCH-BCK-PARA UNTIL SQLCODE = 100         00680000
                    OR K < 1                                            00690000
                EXEC SQL CLOSE OM3 END-EXEC.                            00700000
                PERFORM 3000-WRITEQ-PARA                                00701000
                EXEC CICS RETURN END-EXEC.                              00710000
          3000-WRITEQ-PARA.                                             00720000
                     EXEC CICS WRITEQ TS                                00730000
                              QUEUE(Q-NAME)                             00740000
                              FROM(DLRQ)                                00750000
                              ITEM(CHOICE1)                             00760000
                              REWRITE                                   00770000
                              MAIN                                      00780000
                              RESP(RESCODE)                             00790000
                     END-EXEC                                           00800000
                     IF RESCODE = DFHRESP(NORMAL) CONTINUE              00810000
                     ELSE                                               00820000
                     EXEC CICS WRITEQ TS                                00830000
                              QUEUE(Q-NAME)                             00840000
                              FROM(DLRQ)                                00850000
                              ITEM(CHOICE1)                             00860000
                              MAIN                                      00880000
                              RESP(RESCODE)                             00890000
                     END-EXEC                                           00900000
                     END-IF.                                            00910000
