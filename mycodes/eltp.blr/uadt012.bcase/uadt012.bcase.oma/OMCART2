042600                      CART_CARR_COST = :CART-CARR-COST,           00159100
042610                      CART_STATUS    = :CART-STATUS               00159200
042650                  WHERE CART_CUST_ID = :CART-CUST-ID              00159300
                           AND CART_CART_ID = :CART-CART-ID             00159400
042660             END-EXEC                                             00159500
              DISPLAY ' SQLCODE: ' SQLCODE                              00159600
