//*         DISP=(,PASS),UNIT=&WORK,                                    37600000
//*         DCB=BLKSIZE=400,                                            38400000
//*         SPACE=(400,(400,100))                                       39200000
//*                                                                     40000000
//C        EXEC PGM=&COMPILER,                                          40800000
//         REGION=&REG,                                                 41600000
//         PARM=('&CPARM')                                              43200000
//STEPLIB  DD DSN=&COMPHLQ..&SCBCCMP,DISP=SHR                           44000000
