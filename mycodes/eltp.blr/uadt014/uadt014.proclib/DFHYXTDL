//DFHYXTDL PROC INLIB=,                                                 01400006
//       MEM=,                                                          01401005
//*      INDEX='CICSTS31.CICS',       Qualifier(s) for CICS libraries   01410002
//*      PROGLIB='CICSTS31.CICS.SDFHEXCI', Name of o/p library@BA25137C 02100000
//*      DSCTLIB='CICSTS31.CICS.SDFHC370', Private macro/dsect@BA25137C 02800000
//       COMPILER=CCNDRVR,           C/C++ Compiler OS/390 2.6          03100000
//       COMPHLQ='CBC',             Qualifier(s) for AD/Cycle compiler  03500000
//       LE370HLQ='CEE',             QUALIFIER(S) FOR LE/370 LIBRARIES  04200000
//       OUTC=A,                     Class for print output             04900005
//       REG=4M,                     Region size for all steps          05600000
//       WRKSPC='(3200,(30,30))',    Work SPACE parameter values        06300000
//       DCB80='(RECFM=FB,LRECL=80,BLKSIZE=3200)',                      07000000
//*                                  DCB for LRECL=80 datasets          07700000
//       DCB3200='(RECFM=FB,LRECL=3200,BLKSIZE=12800)',                 08400000
//*                                  DCB for LRECL=3200 datasets        09100000
//       CPARM='RENT,SOURCE',        'C' compiler parameters            09800000
//       LNKPARM='AMODE(31),RMODE(ANY),LIST,MAP,LET,XREF',              11200000
//*                                  Link edit parameters               11700000
//       WORK=SYSDA,                 Unit for work datasets             12200000
//       SCBCCMP=SCCNCMP             Compiler Library                   12500000
//*                                                                     12800000
//*        This procedure is used to translate,compile and linkedit     12900000
//*        C batch application programs using the external cics         13000000
//*        interface (exci).                                            13200000
//*                                                                     13400000
//*        The procedure contains 5 steps                               13600000
//*        1.   Exec the C translator (using the supplied suffix 1$)    14400000
//*        2.   Exec the C compiler                                     15200000
//*        3.   Reblock SDFHMAC(DFHEXLI) for use by the linkedit step   16000000
//*        4.   Exec the C pre-linkedit                                 16800000
//*        5.   Linkedit the output to dataset &PROGLIB                 17600000
//*                                                                     18400000
//*        The following JCL should be used                             19200000
//*        to execute this procedure                                    20000000
//*                                                                     20800000
//*        //APPLPROG EXEC DFHYXTDL                                     21600000
//*        //TRN.SYSIN  DD *                                            22400000
//*           .                                                         23200000
//*           . C application source program                            24000000
//*           .                                                         24800000
//*        /*                                                           25600000
//*        //LKED.SYSIN DD *                                            26400000
//*         NAME anyname(R)                                             27200000
//*        /*                                                           28000000
//*                                                                     28800000
//*      Where   anyname   is the name of your application program.     29600000
//*      (refer to the system definition guide for full details.)       30400000
//*                                                                     32800000
//*TRN      EXEC PGM=DFHEDP&SUFFIX,PARM='EXCI',                         33600000
//*         REGION=&REG                                                 34400000
//*STEPLIB  DD DSN=&INDEX..SDFHLOAD,DISP=SHR                            35200000
